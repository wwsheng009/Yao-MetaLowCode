{
  "name": "自定义接口",
  "version": "1.0.0",
  "description": "自定义API接口",
  "guard": "-",
  "group": "/plugins/metaDataCube/chart",
  "paths": [
    {
      "path": "/queryChartData",
      "method": "post",
      "process": "scripts.plugins.chart.queryChartData",
      "in": [":payload"],
      "out": { "status": 200, "type": "application/json" }
    },
    {
      "path": "/updateDefault",
      "method": "post",
      "process": "scripts.plugins.updateDefault",
      "in": ["$query.id", "$query.defaultChart"],
      "out": { "status": 200, "type": "application/json" }
    }
  ]
}
